<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="BevListaSQL.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:BevListaSQL"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <Tab Title="Új felvétel" Icon="{StaticResource Plus}">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
        </Tab>
        <Tab Title="Lista" Icon="{StaticResource List}">
            <ShellContent ContentTemplate="{DataTemplate local:ListPage}" />
        </Tab>
    </TabBar>

</Shell>
